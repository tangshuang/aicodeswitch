{
  "name": "aicodeswitch",
  "version": "3.0.7",
  "description": "A tool to help you manage AI programming tools to access large language models locally. It allows your Claude Code, Codex and other tools to no longer be limited to official models.",
  "author": "tangshuang",
  "license": "GPL-3.0",
  "homepage": "https://github.com/tangshuang/aicodeswitch",
  "repository": {
    "type": "git",
    "url": "https://github.com/tangshuang/aicodeswitch.git"
  },
  "bugs": {
    "url": "https://github.com/tangshuang/aicodeswitch/issues"
  },
  "keywords": [
    "ai",
    "code",
    "switch",
    "claude",
    "codex",
    "proxy",
    "api",
    "programming",
    "development",
    "typescript",
    "express",
    "react"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/server/main.js",
  "bin": {
    "aicos": "./bin/cli.js",
    "aicodeswitch": "./bin/cli.js",
    "aicos-migrate": "./bin/migrate.js"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:ui\"",
    "dev:ui": "vite",
    "dev:server": "tsx --tsconfig=tsconfig.server.json src/server/main.ts",
    "dev:server:watch": "tsx watch --tsconfig=tsconfig.server.json --clear-screen=false --watch-kill-signal=SIGINT src/server/main.ts",
    "build": "npm run build:ui && npm run build:server",
    "build:ui": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepublishOnly": "npm run build",
    "release": "standard-version --no-changelog",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build && node tauri/move-bundle.js",
    "tauri:icon": "tauri icon src/ui/assets/logo.png"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.6.7",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.3",
    "ora": "^5.4.1",
    "ws": "^8.19.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^11.8.1",
    "level": "^8.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "dayjs": "^1.11.19",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.21.3",
    "recharts": "^3.7.0",
    "standard-version": "^9.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
